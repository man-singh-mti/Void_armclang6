    "string_view": "cpp",
    "vector": "cpp",
    "xstring": "cpp",
    "xutility": "cpp",
    "mti_radar.h": "c",
    "stdio.h": "c",
    "stddef.h": "c",
    "mti_system.h": "c",
    "string.h": "c",
    "__locale": "c",
    "cstddef": "c",
    "deque": "c",
    "ios": "c",
    "limits": "c",
    "locale": "c",
    "new": "c",
    "variant": "c",
    "bitset": "c",
    "istream": "c",
    "memory": "c",
    "streambuf": "c",
    "atomic": "c",
    "bit": "c",
    "charconv": "c",
    "chrono": "c",
    "compare": "c",
    "execution": "c",
    "format": "c",
    "functional": "c",
    "iterator": "c",
    "mutex": "c",
    "sstream": "c",
    "system_error": "c",
    "type_traits": "c",
    "xhash": "c",
    "xiosbase": "c",
    "xlocale": "c",
    "xlocbuf": "c",
    "xlocmon": "c",
    "xloctime": "c",
    "xmemory": "c",
    "math.h": "c",
    "mti_radar_types.h": "c",
    "mti_can.h": "c",
    "cmath": "c",
    "cstdlib": "c",
    "ratio": "c",
    "xlocnum": "c"
  },
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#828282",
    "activityBar.background": "#828282",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#70cfcf",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#828282",
    "statusBar.background": "#696969",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#828282",
    "statusBarItem.remoteBackground": "#696969",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#696969",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#69696999",
    "titleBar.inactiveForeground": "#e7e7e799"
},
  "[xml]": {
    "editor.defaultFormatter": "redhat.vscode-xml"
},
"[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
}
}
